{
  "parser"  : "babel-eslint",
  "extends" : [
    "standard",
  ],
  "env"     : {
    "browser" : true
  },
  "rules": {
    "indent": 0,

    "comma-dangle": [1, "always-multiline"],
    "key-spacing": 1,
    "no-unused-vars": 1,
    "no-use-before-define": 1,
    "object-curly-spacing": [1, "always"],
    "semi" : [2, "never"],
    "space-before-function-paren": [1, "never"]
  }
}
